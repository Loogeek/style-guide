#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/long.xiao/Documents/Github/eslint-config/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/long.xiao/Documents/Github/eslint-config/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../registry.npmmirror.com+ts-node@10.9.1_yodorn5kzjgomblrsstrk2spaa/node_modules/ts-node/dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../../../../../registry.npmmirror.com+ts-node@10.9.1_yodorn5kzjgomblrsstrk2spaa/node_modules/ts-node/dist/bin-cwd.js" "$@"
fi
